<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express Lab Skills</title>
</head>
<body>
  <h1>Skills</h1>
  <ul>
    <a href="/skills/new">Add New Skill</a>
    <% Skills.forEach(function(skill) => { %>
      <li>
				<a href="/skills/<%= skill._id %>"><%= skill.text %></a>
			</li>
    <% }) %>
  </ul>
</body>
</html>